generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Orcamento {
  id           String   @id @default(uuid())
  nomeCliente  String
  endereco     String
  telefone     String
  itens        Json
  createdAt    DateTime @default(now())
}
